package com.jpm.otm.entity;

import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name="DEPT_OTM")
public class Department {
	
	@Id
	@Column(name="dept_id")
	private Long deptId;
	
	@Column(name="dept_name")
	private String deptName;
	
	public Department() {
		
	}

}
